{
  "name": "labs",
  "version": "1.0.0",
  "description": "To test protractor with typescript",
  "main": "config.js",
  "scripts": {
    "ts-build": "node_modules/.bin/tsc",
    "wdm-start": "node ./node_modules/protractor/node_modules/.bin/webdriver-manager start",
