{
  "name": "labs",
  "version": "1.0.0",
  "description": "To automate tests with protractor and typescript",
  "scripts": {
    "wdm-start": "node_modules/.bin/webdriver-manager start",
    "wdm-update": "node_modules/.bin/webdriver-manager update",
    "run-wdm": "npm run wdm-update && npm run wdm-start",
    "run-del": "shx rm -rf JSFiles",
    "ts-build": "./node_modules/.bin/tsc",
    "pretest": "npm run run-del && npm run ts-build",
    "test": "protractor JSFiles/configuration.js"
  },
  "keywords": [
    "angular-protractor-labs",
    "protractor-cucumber-automation",
    "protractor-cucumber-reporter"
  ],
  "author": "Srinivasan K K",
  "license": "ISC",
  "dependencies": {
    "protractor": "^7.0.0",
    "jasmine": "^3.7.0",
    "typescript": "^4.2.4",
    "protractor-cucumber-framework": "8.0.4",
    "@cucumber/cucumber": "^7.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^15.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cucumber-html-reporter": "^5.4.0",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1"
  }
}
