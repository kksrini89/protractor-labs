  "keywords": [
    "angular-protractor-labs"
  ],
  "author": "Srinivasan K K",
  "license": "ISC",
  "dependencies": {
    "protractor": "^7.0.0",
    "jasmine": "^3.7.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^15.0.1",
    "ts-node": "^9.1.1"
  }
}
